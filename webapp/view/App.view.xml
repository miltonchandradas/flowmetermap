<mvc:View
    controllerName="com.aqua.h2oflowmetermap.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
					<HBox id="map_canvas" fitContainer="true" justifyContent="Center" alignItems="Center" >
					</HBox>
				</content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
